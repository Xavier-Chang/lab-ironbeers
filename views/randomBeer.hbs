<div class="randombeer-page">
   {{#each randomBeer}}
   {{> beerpartial}}  
   
   <div class="randombeer-info">
      <h3>Food pairing:</h3>
     <ul>
         {{#each food_pairing}}
            <li>{{this}}</li>
         {{/each}}
     </ul>
     
     <h3>Brewers tips:</h3>
     <p>{{this.brewers_tips}}</p>
   </div>
   
   {{/each}}
</div>
